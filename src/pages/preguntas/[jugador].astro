---
import { Juego } from "@/components/preguntas/Juego";
import Layout from "@/layouts/Layout.astro";

export const prerender = true;

export function getStaticPaths() {
  return [{ params: { jugador: "amigos" } }, { params: { jugador: "pareja" } }];
}

const { jugador } = Astro.params;
---

<Layout title="Preguntas | Tirale a tu ex">
  <Juego modo={jugador} client:load/>
</Layout>
